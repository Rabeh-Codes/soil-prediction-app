{
  "name": "soil-prediction-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc -b && npx vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0 --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:ci": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0 --format junit -o reports/eslint-report.xml",
    "lint:staged": "lint-staged",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:ci": "prettier --check --loglevel warn \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --list-different",
    "prepare": "if [ \"$HUSKY\" != \"0\" ]; then npx husky install; fi",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --quiet --max-warnings 0",
      "prettier --write --loglevel warn"
    ],
    "*.{js,jsx}": [
      "eslint --fix --quiet --max-warnings 0",
      "prettier --write --loglevel warn"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write --loglevel warn"
    ],
    "*.{json,md,html}": [
      "prettier --write --loglevel warn"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "stylelint-high-performance-animation": "^1.11.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^5.0.1"
  }
}
